{
  "name": "base-nestjs-mongo",
  "version": "1.0.1",
  "description": "Base repository nestjs with mongodb",
  "author": "VuThanhThien",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "npx prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "start": "NODE_ENV=development && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "test": "jest --runInBand --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "cron:send-email": "nest build && node dist/modules/cronjob/isolate-cronjob/email-cronjob/send-email-cron.exec.js"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "api-spec-converter": "^2.12.0",
    "bignumber.js": "^9.1.1",
    "bluebird": "^3.7.2",
    "bull": "^4.10.4",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^4.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.0.4",
    "multer": "^1.4.5-lts.1",
    "nestjs-pino": "^3.1.3",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "redis": "^4.6.6",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "^7.2.0",
    "swagger-client": "^3.19.6",
    "uuid": "^9.0.0",
    "verror": "^1.10.1",
    "web-push": "^3.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bull": "^4.10.0",
    "@types/config": "^3.3.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "18.15.11",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "husky": "^8.0.0",
    "jest": "29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,tsx,json,scss,md,html}": [
      "eslint",
      "npm run format",
      "git add"
    ]
  }
}
